{
  "name": "@ts-starter/crud",
  "version": "1.1.0",
  "private": true,
  "description": "Nestjs CRUD module",
  "author": "Joseph Cheng <joseph.cheng@yourevercare.com>",
  "engines": {
    "node": ">=12 <13"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "env $(cat .env.local | grep -v \"#\" | xargs) nodemon --config ./nodemon.json",
    "build": "tsc --build ./tsconfig.build.json",
    "prebuild": "yarn clean",
    "predev": "yarn build",
    "start": "node dist/main.js",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "clean": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "typeorm": "node --require ts-node/register ../../node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "7.1.3",
    "@nestjs/core": "7.1.3",
    "@nestjs/platform-express": "7.1.3",
    "@nestjs/typeorm": "7.1.0",
    "@ts-starter/types": "1.1.0",
    "class-sanitizer": "0.0.5",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "express": "4.17.1",
    "module": "^1.2.5",
    "reflect-metadata": "0.1.13",
    "sqlite3": "4.2.0",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@nestjs/testing": "7.1.3",
    "@types/compression": "1.7.0",
    "@types/express-rate-limit": "5.0.0",
    "@types/helmet": "0.0.47",
    "@types/node": "14.0.12",
    "nodemon": "2.0.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  }
}
